@{
    ViewData["Title"] = "Home Page";
}

@if (AppUser.UserIsAuthenticated(this.Context))
{
    <aswd-main></aswd-main>
}
else
{
    <aswd-login></aswd-login>
}

@section subelements
{
@if (AppUser.UserIsAuthenticated(this.Context))
{
    @Html.Raw("<link rel='import' href='/Elements/Load/Main/" + AppUser.IDAppUser(this.Context) + "'>")
}
else
{
    <link rel="import" href="~/elements/aswd-login.html">
}
}
