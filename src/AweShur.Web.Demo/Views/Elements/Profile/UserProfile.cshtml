@{
    Layout = "";

    int? idappuser = Context.Session.GetInt32("idappuser");
}
<link rel="import" href="/lib/polymer/polymer.html">
<link rel="import" href="/elements/lib/awsh-editor/awsh-editor.html">

<dom-module id="swd-profile">
    <template>
        <style>
        </style>

        <awsh-editor id="editor" oname="AppUserProfile" idobject="@idappuser" on-elementready="_elementready" hiderefresh="true" hideadd="true"
                     edithref='@("/Elements/UserProfileEdit/" + idappuser)' editelement="swd-userprofileedit">
        </awsh-editor>
    </template>
    <script>
        Polymer({
            is: 'swd-profile',
            properties: {
            },
            _elementready: function () {
                this.$.editor.load();
            }
        });
    </script>
</dom-module>
