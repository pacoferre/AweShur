@model BusinessBaseDecorator
@{
    Layout = "_CRUDLayout";
}

<link rel="import" href="~/lib/paper-item/paper-item.html">

<dom-module id="aswd-customercontactedit">
    <template>
        <style include="shared-styles"></style>
        <paper-item>
            @Html.PolEditor(Model, "name")
            @Html.PolEditor(Model, "surname")
        </paper-item>
    </template>
    <script>
    Polymer({
        is: 'aswd-customercontactedit',
        behaviors: [AWSLBehaviors.ElementBehavior],
    });
    </script>
</dom-module>