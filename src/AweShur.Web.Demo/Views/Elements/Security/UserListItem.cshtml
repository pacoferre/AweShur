@{
    Layout = "";
}
<link rel="import" href="/lib/polymer/polymer.html">

<dom-module id="aswd-userlistitem">
    <template>
        <style>
            .primary {
                font-size: 16px;
                display: block;
            }
        </style>

        <div class="primary">
            [[item.name]] [[item.surname]]
        </div>
    </template>
    <script>
        Polymer({
            is: 'aswd-userlistitem',
            properties: {
                item: {
                    type: Object,
                },
            },
        });
    </script>
</dom-module>